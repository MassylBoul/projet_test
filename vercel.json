{
  "version": 2,
  "builds": [
    {
      "src": "app/**/*",
      "use": "@vercel/node"
    },
    {
      "src": "next.config.js",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/layout.tsx"
    },
    {
      "src": "/(.*)",
      "dest": "/app/$1"
    },
    {
      "src": "/404",
      "dest": "/app/404.tsx"
    }
  ]
}
